'From Squeak5.3 of 25 January 2021 [latest update: #19458] on 12 April 2021 at 12:22:31 am'!Object subclass: #Islemler	instanceVariableNames: ''	classVariableNames: ''	poolDictionaries: ''	category: 'Islem'!"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Islemler class	instanceVariableNames: ''!!Islemler class methodsFor: 'as yet unclassified' stamp: 'SC 4/11/2021 20:06'!ciftYazdir: koleksiyon	"Parametre olarak verilen koleksiyndaki cift elemanlari Transcript ekranina alt alta yazdiran metot"	koleksiyon do: [:e|		(e rem: 2) = 0		ifTrue: [Transcript show: e; cr.]	].! !!Islemler class methodsFor: 'as yet unclassified' stamp: 'SC 4/11/2021 20:28'!enKucuk: koleksiyon	"Parametre olarak verilen koleksiyondaki en kucuk elemani veren metot"	^koleksiyon inject: (koleksiyon atRandom) into: [:d1 :d2| d1 min: d2].		! !!Islemler class methodsFor: 'as yet unclassified' stamp: 'SC 4/11/2021 19:59'!tekTopla: sayi	"1 ile n arasindaki tek sayilarin toplamini veren metot"	|toplam|	toplam := 0.	1 to: sayi do: [:i | 		(i rem: 2) = 1			ifTrue: [toplam := toplam + i]		].	^toplam! !!Islemler class methodsFor: 'as yet unclassified' stamp: 'SC 4/11/2021 20:14'!tekler: koleksiyon	"Parametre olarak verilen koleksiyondaki tek elemanlari veren metot"	^koleksiyon select: [:e| (e rem: 2) = 1]! !!Islemler class methodsFor: 'as yet unclassified' stamp: 'SC 4/11/2021 20:10'!topla: koleksiyon	"Parametre olarak verilen koleksiyondaki elemanlarin toplamini veren metot"	| toplam |	toplam := 0.	koleksiyon do: [:e| toplam := toplam + e].	^toplam	! !